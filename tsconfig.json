{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": false,
    "noEmit": false,
    "noEmitOnError": false
  },
  "include": ["src"]
}
